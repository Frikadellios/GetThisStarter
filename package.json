{
  "name": "create-astro-starter",
  "type": "module",
  "version": "0.13.4",
  "author": "Giulio Zanchetta",
  "description": "Astro starter template with tailwind, alpinejs, localization, blog, CMS, and comments",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Frikadellios/GetThisStarter.git"
  },
  "bugs": "https://github.com/Frikadellios/GetThisStarter/issues",
  "homepage": "https://github.com/Frikadellios/GetThisStarter",
  "files": [
    "*"
  ],
  "scripts": {
    "dev": "astro dev",
    "start": "cd node_modules/sharp && bun install && cd ../../ && bun run build",
    "build": "npm run pwa:generate-assets && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "pwa:generate-assets": "pwa-assets-generator --preset minimal public/logo.svg"
  },
  "dependencies": {
    "@astrojs/alpinejs": "^0.3.1",
    "@astrojs/cloudflare": "^8.0.0",
    "@astrojs/mdx": "2.0.1",
    "@astrojs/partytown": "^2.0.2",
    "@astrojs/react": "^3.0.7",
    "@astrojs/rss": "^4.0.1",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.3",
    "@astrojs/ts-plugin": "^1.3.1",
    "@fontsource-variable/inter": "^5.0.16",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/alpinejs": "^3.13.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "alpinejs": "^3.13.3",
    "astro": "4.0.6",
    "astro-i18n-aut": "^0.5.0",
    "astro-sveltia-cms": "^0.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.298.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@vite-pwa/assets-generator": "^0.0.11",
    "astro-icon": "^0.8.2",
    "astro-seo": "^0.8.0",
    "bun-types": "^1.0.18",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "prettier": {
    "tabWidth": 4,
    "useTabs": true,
    "bracketSameLine": true,
    "printWidth": 150,
    "htmlWhitespaceSensitivity": "ignore",
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss"
    ]
  }
}
